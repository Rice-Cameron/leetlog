{
  "name": "leetlog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:db": "dotenv -e .env.test.local -e .env -- sh -c 'DATABASE_URL=$DATABASE_URL_TEST npx prisma db push'",
